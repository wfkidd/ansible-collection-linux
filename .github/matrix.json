{
  "ansible": ["stable-2.9", "stable-2.10"],
  "distro": [
    "centos/7/cloud",
    "debian/11/cloud",
    "fedora/34/cloud",
    "ubuntu/20.04/cloud"
  ],
  "include": [
    {
      "ansible": "devel",
      "distro": "ubuntu/20.04/cloud"
    }
  ],
  "lxd": {
    "exclude": [
      { "distro": "debian/11/cloud" },
      { "distro": "fedora/34/cloud" }
    ]
  },
  "adguardhome": {
    "include": [
      {
        "ansible": "devel",
        "distro": "ubuntu/20.04/cloud",
        "install_method": "snap"
      },
      {
        "ansible": "stable-2.9",
        "distro": "ubuntu/18.04/cloud",
        "install_method": "docker"
      }
    ]
  },
  "traefik": {
    "include": [
      {
        "ansible": "devel",
        "distro": "ubuntu/20.04/cloud",
        "install_method": "docker"
      }
    ]
  },
  "snapd": {
    "include": [
      {
        "ansible": "stable-2.10",
        "distro": "amazonlinux"
      }
    ]
  }
}
